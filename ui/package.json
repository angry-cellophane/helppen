{
    "name": "helppen-web",
    "version": "1.0.0",
    "description": "HelpPen web app",
    "main": "index.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "rimraf": "rimraf",
        "webdriver-update": "webdriver-manager update",
        "webdriver-start": "webdriver-manager start",
        "postinstall": "typings install && npm run webdriver-update",
        "clean": "npm cache clean && npm run rimraf -- doc bundles typings coverage node_modules",
        "clean-install": "npm run clean && npm install",
        "e2e": "protractor",
        "e2e-live": "protractor --elementExplorer",
        "hp:watch": "rimraf bundles && webpack --progress --profile --bail --watch",
        "hp:dev": "rimraf bundles && webpack --progress --profile --bail",
        "hp:production": "rimraf bundles && webpack --production --progress --profile --bail",
        "hp:server": "rimraf bundles && webpack --progress --profile --bail && webpack-dev-server --inline --progress",
        "hp:server2": "rimraf bundles && webpack --progress --profile --bail --port 8089 && webpack-dev-server --inline --progress --port 8089",
        "hp:production:server": "rimraf bundles && webpack --production --progress --profile --bail && webpack-dev-server --inline --progress --port 8089"
    },
    "license": "ISC",
    "dependencies": {
        "@angular/common": "2.0.0-rc.4",
        "@angular/compiler": "2.0.0-rc.4",
        "@angular/core": "2.0.0-rc.4",
        "@angular/forms": "0.2.0",
        "@angular/http": "2.0.0-rc.4",
        "@angular/platform-browser": "2.0.0-rc.4",
        "@angular/platform-browser-dynamic": "2.0.0-rc.4",
        "@angular/router": "3.0.0-beta.2",
        "@angular/router-deprecated": "2.0.0-rc.2",
        "@angular/upgrade": "2.0.0-rc.4",
        "core-js": "^2.4.0",
        "font-awesome": "^4.6.3",
        "ng2-translate": "2.2.2",
        "reflect-metadata": "^0.1.3",
        "rxjs": "5.0.0-beta.6",
        "zone.js": "^0.6.12"
    },
    "devDependencies": {
        "autoprefixer": "^6.3.6",
        "copy-webpack-plugin": "^3.0.0",
        "css-loader": "^0.23.1",
        "extract-text-webpack-plugin": "^1.0.1",
        "file-loader": "^0.8.5",
        "html-webpack-plugin": "^2.17.0",
        "istanbul-instrumenter-loader": "^0.2.0",
        "jasmine-core": "^2.4.1",
        "jasmine-spec-reporter": "^2.4.0",
        "karma": "^0.13.22",
        "karma-chrome-launcher": "^1.0.1",
        "karma-coverage": "^1.0.0",
        "karma-jasmine": "^1.0.2",
        "karma-phantomjs-launcher": "^1.0.0",
        "karma-sourcemap-loader": "^0.3.7",
        "karma-spec-reporter": "0.0.26",
        "karma-webpack": "^1.7.0",
        "minimist": "^1.2.0",
        "node-sass": "^3.7.0",
        "phantomjs-prebuilt": "^2.1.7",
        "postcss-loader": "^0.9.1",
        "protractor": "^3.3.0",
        "raw-loader": "^0.5.1",
        "rimraf": "^2.5.2",
        "sass-loader": "^3.2.0",
        "style-loader": "^0.13.1",
        "ts-loader": "^0.8.2",
        "tslint": "^3.10.2",
        "tslint-loader": "^2.1.4",
